        android:layout_height="match_parent"
        android:id="@+id/piechart"
        app:layout_constraintLeft_toLeftOf="parent"
        android:padding="10dp">
    </com.github.mikephil.charting.charts.PieChart>

    <com.github.mikephil.charting.charts.PieChart
        android:layout_width="205dp"
